{
  "schema_version": 1,
  "quilt_loader": {
    "group": "net.frozenblock",
    "id": "wilderwild",
    "version": "${version}",
    "metadata": {
      "name": "Wilder Wild",
      "description": "Wilder Wild overhauls the Wild Update, adding features and improving existing ones.",
      "contributors": {
        "FrozenBlock": "Owner"
      },
      "contact": {
        "homepage": "https://www.curseforge.com/minecraft/mc-mods/wilder-wild",
        "issues": "https://github.com/Soro335/WilderWild/issues",
        "sources": "https://github.com/Soro335/WilderWild/"
      },
      "icon": "assets/wilderwild/icon.png"
    },
    "intermediate_mappings": "net.fabricmc:intermediary",
    "license": "LGPL-3.0",
    "environment": "*",
    "entrypoints": {
      "init": "net.frozenblock.wilderwild.WilderWild",
      "client_init": "net.frozenblock.wilderwild.WilderWildClient",
      "mm:early_risers": "net.frozenblock.wilderwild.EarlyRiser",
      "modmenu": "net.frozenblock.wilderwild.misc.config.ModMenuIntegration",
      "events": [
        "net.frozenblock.wilderwild.world.WilderWildSurfaceRules"
        ]
    },
    "accessWidener": "wilderwild.accesswidener",
    "depends": [
      {
        "id": "mm",
        "versions": ">=2.3"
      },
      {
        "id": "quilt_loader",
        "versions": ">=0.17.1-"
      },
      {
        "id": "quilted_fabric_api",
        "versions": ">=4.0.0-"
      },
      {
        "id": "minecraft",
        "versions": [
          "1.19.1",
          "1.19.2"
        ]
      },
      {
        "id": "java",
        "versions": ">=17"
      }
    ]
  },
  "mixin": "wilderwild.mixins.json"
}
